{
  "family": "paper-portal-dev",
  "containerDefinitions": [
    {
      "name": "paper-portal-dev",
      "image": "440551823509.dkr.ecr.us-east-1.amazonaws.com/paper-portal-dev:latest",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "command": [
        "/bin/bash",
        "-lc",
        ". .venv/bin/activate && python app/main.py"
      ],
      "environment": [
        {
          "name": "ENV_TYPE",
          "value": "dev"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "DB_LOGIN",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:440551823509:secret:rds!db-8a461da8-14cb-498d-accc-dd0d90c0d78e-yjUlEX"
        },
        {
          "name": "DB_ADDRESS",
          "valueFrom": "arn:aws:ssm:us-east-1:440551823509:parameter/dev/db-address"
        },
        {
          "name": "DB_NAME",
          "valueFrom": "arn:aws:ssm:us-east-1:440551823509:parameter/dev/db-name"
        },
        {
          "name": "DB_PORT",
          "valueFrom": "arn:aws:ssm:us-east-1:440551823509:parameter/dev/db-port"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/paper-portal-dev",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "executionRoleArn": "arn:aws:iam::440551823509:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
